 className=\"flex\">\n        <div>\n          <h3>{item.name}</h3>\n        </div>\n        <div className=\"price\">\n          <h3>\n          {item.original_price&&<span className=\"left-margin gray\"><strike>{`$ ${item.original_price} `}</strike></span>}\n          <span>{\"$ \"+item.final_price}</span>\n          </h3>\n        </div>\n      </div>\n      <div className=\"item-description\">{item.description}</div>\n      {quantity !== 0 ? (\n        <div className=\"border-blue\">\n          <div>\n            <button onClick={() => dispatch(removeItemFromCart(item.id))} className=\"blue button\">\n              -\n            </button>\n          </div>\n          <div className=\"addtocart center\">{quantity}</div>\n          <div>\n            <button onClick={() => dispatch(addItemToCart(item.id))} className=\"blue button\">\n              +\n            </button>\n          </div>\n        </div>\n      ) : (\n        <div className=\"border-blue\">\n          <button className=\"addtocart button\" onClick={() => dispatch(addItemToCart(item.id))} >\n            Add to Cart\n          </button>\n        </div>\n      )}\n    </div>\n  );\n}\n\nexport const mapStateToProps =(state)=>({\n  state\n});\n\nexport default connect(mapStateToProps)(Cart);","/home/vinod/Desktop/cart-app/src/redux/store.jsx",[],"/home/vinod/Desktop/cart-app/src/redux/reducer.jsx",[],"/home/vinod/Desktop/cart-app/src/redux/actionTypes.jsx",[],"/home/vinod/Desktop/cart-app/src/redux/actions.jsx",[],"/home/vinod/Desktop/cart-app/src/components/CartItems.jsx",[],"/home/vinod/Desktop/cart-app/src/components/CartSummery/CartSummery.jsx",["56","57","58","59"],"import \"./summery.scss\";\nimport React from \"react\";\nimport { connect } from \"react-redux\";\nimport { Link } from \"react-router-dom\";\nimport { mapStateToProps } from \"../Cart\";\nimport data from \"../data\";\nimport {\n  addItemToCart,\n  removeItemFromCart,\n  removeWholeItemFromCart,\n} from \"../../redux/actions\";\n\nfunction CartSummery(props) {\n  let total = 0;\n  let save=0;\n\n  return (\n    <div>\n      <div className=\"wrap cf\">\n        <h1 className=\"projTitle\">Order Summary</h1>\n        <div className=\"heading cf\">\n          <h1>My Cart</h1>\n          <Link to=\"/\" className=\"continue\">\n            + Add more items\n          </Link>\n        </div>\n        <div className=\"cart\">\n          <ul className=\"cartWrap\">\n            {props.state.map((itm, idx) => {\n\n              //calculating discount and total sum\n\n              total +=\n                data.find((item) => itm.id === item.id).final_price *\n                itm.quantity;\n              if(data.find((item) => itm.id === item.id).original_price){\n                  save+=(data.find((item) => itm.id === item.id).original_price-data.find((item) => itm.id === item.id).final_price)*itm.quantity;\n              }\n\n              return (\n                <li className=\"items odd\">\n                  <div className=\"infoWrap\">\n                    <div className=\"cartSection\">\n                      <img\n                        src={data.find((item) => itm.id === item.id).img_url}\n                        alt={data.find((item) => itm.id === item.id).name}\n                        className=\"itemImg\"\n                      />\n                      <p className=\"itemNumber\">#QUE-007544-00{itm.id}</p>\n                      <h3>{data.find((item) => itm.id === item.id).name}</h3>\n                      <p>\n                        {\" \"}\n                        <button\n                          onClick={() =>\n                            props.dispatch(removeItemFromCart(itm.id))\n                          }\n                        >\n                          -\n                        </button>\n                        <span>{itm.quantity}</span>\n                        <button\n                          onClick={() => props.dispatch(addItemToCart(itm.id))}\n                        >\n                          +\n                        </button>\n                        x{`$${data.find((item) => itm.id === item.id).final_price}.00`}\n                      </p>\n\n                      <p className=\"stockStatus\"> In Stock</p>\n                    </div>\n\n                    <div className=\"prodTotal cartSection\">\n                      <p>\n                        {`$${data.find((item) => itm.id === item.id).final_price}.00`}\n                      </p>\n                    </div>\n                    <div\n                      className=\"cartSection removeWrap\"\n                      onClick={() =>\n                        props.dispatch(removeWholeItemFromCart(itm.id))\n                      }\n                    >\n                      <a href=\"#\" className=\"remove\">\n                        x\n                      </a>\n                    </div>\n                  </div>\n                </li>\n              );\n            })}\n          </ul>\n        </div>\n\n        <div className=\"promoCode\">\n          <label for=\"promo\">Have A Promo Code?</label>\n          <input type=\"text\" name=\"promo\" placholder=\"Enter Code\" />\n          <a href=\"#\" className=\"btn\"></a>\n        </div>\n\n        <div className=\"subtotal cf\">\n          <ul>\n            <li className=\"totalRow\">\n              <span className=\"label\">Subtotal</span>\n              <span className=\"value\">{`$${total}.00`}</span>\n            </li>\n\n            <li className=\"totalRow\">\n              <span className=\"label\">Total Savings</span>\n              <span className=\"value\">\n                {props.state.length === 0 ? `$0.00` : `$${save}.00`}\n              </span>\n            </li>\n\n            <li className=\"totalRow\">\n              <span className=\"label\">Shipping</span>\n              <span className=\"value\">\n                {props.state.length === 0 ? `$0.00` : \"$5.00\"}\n              </span>\n            </li>\n\n            <li className=\"totalRow\">\n              <span className=\"label\">Tax</span>\n              <span className=\"value\">\n                {props.state.length === 0 ? `$0.00` : `$4.00`}\n              </span>\n            </li>\n            \n\n            <li className=\"totalRow\">\n              <div className=\"totalRow final\">\n                <span className=\"label\">Total</span>\n                <span className=\"value\">\n                  {props.state.length === 0 ? `$0.00` : `${total + 9}.00`}\n                </span>\n              </div>\n              <a href=\"#\" className=\"btn continue\">\n                Checkout\n              </a>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default connect(mapStateToProps)(CartSummery);\n",{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":1,"message":"65","line":14,"column":7,"nodeType":"66","endLine":14,"endColumn":66},{"ruleId":"67","severity":1,"message":"68","line":83,"column":23,"nodeType":"66","endLine":83,"endColumn":54},{"ruleId":"69","severity":1,"message":"70","line":97,"column":11,"nodeType":"66","endLine":97,"endColumn":39},{"ruleId":"67","severity":1,"message":"68","line":97,"column":11,"nodeType":"66","endLine":97,"endColumn":39},{"ruleId":"67","severity":1,"message":"68","line":136,"column":15,"nodeType":"66","endLine":136,"endColumn":52},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a1